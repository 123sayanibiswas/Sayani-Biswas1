import java.util.HashMap;
import java.util.Map;

public class Car {
	String name;
	int price;
	String color;
	public Car() {
		// TODO Auto-generated constructor stub
	}
	Car(String name, int price, String color)
	{
		this.name=name;
		this.price=price;
		this.color=color;
	}


	Map<String, Car> carMap=new HashMap<String, Car>();
	

}
