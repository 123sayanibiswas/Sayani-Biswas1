package calculate;

public class calculate {
	public static void main(String[]args)
	{
	
		System.out.println("Enter the values");
		int num1=20;
		int num2=30;
		int a=num1+num2;
		int b=num2-num1;
		int c=num2/num1;
		int d=num1*num2;
		System.out.println("Enter the addition of" +a);
		System.out.println("Enter the substraction of" +b);
		System.out.println("Enter the division of"  +c);
		System.out.println("Enter the multipication of" +d);

	}

}

